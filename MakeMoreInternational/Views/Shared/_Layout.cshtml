                    $('.head').addClass('sticky-header');
                    $('.bar-icon').css("color", "black");
                } else {
                    $("#back-to-top").css("right", "-90px");
                    $('header').css('position', 'absolute');
                    $('.head').removeClass('sticky-header');
                    $('.bar-icon').css("color", "white");
                }
            })
        });
    </script>

