    <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>
    <script>
        const map = L.map('map').setView([20, 0], 2); // Centered view of the world

        // Add OpenStreetMap tiles
        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            attribution: ''
        }).addTo(map);

        // Highlighted countries with colors
        const highlightCountries = [{
            name: "India",
            color: "orange"
        }, {
            name: "United States of America",
            color: "pink"
        }, {
            name: "United Kingdom",
            color: "red"
        }, {
            name: "Japan",
            color: "#9c27b0"
        }, {
            name: "Turkey",
            color: "yellow"
        }];

        // Fetch world GeoJSON and apply style for selected countries
        fetch('https://raw.githubusercontent.com/johan/world.geo.json/master/countries.geo.json')
            .then(res => res.json())
            .then(data => {
                L.geoJSON(data, {
                    style: feature => {
                        const country = highlightCountries.find(c => c.name === feature.properties.name);
                        if (country) {
                            return {
                                color: country.color,
                                weight: 1,
                                fillColor: country.color,
                                fillOpacity: 0.2
                            };
                        }
                        return {
                            color: "#ccc",
                            weight: 1,
                            fillOpacity: 0
                        };
                    },
                    onEachFeature: (feature, layer) => {
                        const country = highlightCountries.find(c => c.name === feature.properties.name);
                        if (country) {
                            layer.bindPopup(`<b>${country.name}</b>`);
                        }
                    }
                }).addTo(map);
            });
    </script>

