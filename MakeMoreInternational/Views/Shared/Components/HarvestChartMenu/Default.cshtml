@model List<HarvestCategoryMaster>

<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" href="#" id="harvestMenu"
       role="button" data-bs-toggle="dropdown" aria-expanded="false">
        HARVEST CHART
    </a>

    <ul class="dropdown-menu" aria-labelledby="harvestMenu">
        @foreach (var cat in Model)
        {
            <li>
                <a class="dropdown-item"
                   href="/harvest-chart/@cat.hcatSlug">
                    @cat.hcatName
                </a>
            </li>
        }
    </ul>
</li>
